{
  "name": "docker-at-rest",
  "version": "0.1.3",
  "type": "module",
  "description": "Docker manager utilities and server using a REST API",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build",
    "start": "node ./dist/cli.js",
    "dev": "ts-node src/cli.ts"
  },
  "bin": {
    "docker-at-rest": "./dist/cli.js"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "docker",
    "manager"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "winston": "^3.18.3",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/yargs": "^17.0.35",
    "typescript": "^5.9.3"
  }
}
